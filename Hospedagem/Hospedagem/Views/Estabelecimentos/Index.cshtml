@model IEnumerable<Hospedagem.Models.Estabelecimento>

@{
    ViewBag.Title = "Index";
}

<h4>  Estabelecimentos</h4>
@Html.ActionLink("Adicionar", "Inserir")
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.IDEstab)</th>
        <th>@Html.DisplayNameFor(model => model.IDCidade)</th>
        <th>@Html.DisplayNameFor(model => model.IDCategoria)</th>
        <th>@Html.DisplayNameFor(model => model.Nome)</th>
        <th>@Html.DisplayNameFor(model => model.Endereco)</th>
        <th>@Html.DisplayNameFor(model => model.Telefone)</th>
        <th>@Html.DisplayNameFor(model => model.Site)</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.IDEstab</td>
            <td>@item.IDCidade</td>
            <td>@item.IDCategoria</td>
            <td>@item.Nome</td>
            <td>@item.Endereco</td>
            <td>@item.Telefone</td>
            <td>@item.Site</td>
            <td>
                @Html.ActionLink("Alterar", "Alterar", new { id = item.IDEstab }) |
                @Html.ActionLink("Excluir", "Excluir", new { id = item.IDEstab })
            </td>
        </tr>
    }
</table>